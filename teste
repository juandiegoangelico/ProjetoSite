import pyautogui
import time
import pyperclip


from selenium import webdriver
from selenium.webdriver.common.keys import Keys
from selenium.webdriver.common.by import By

pyautogui.PAUSE = 1


pyautogui.hotkey("Ctrl", "Shift", "N")


link = 'https://www.angelicoanziutti.com/post/lgpd-dado-pessoal-sens%C3%ADvel-no-%C3%A2mbito-trabalhista'
pyperclip.copy(link)
pyautogui.hotkey("Ctrl", "v")
pyautogui.press ("Enter")

pyautogui.PAUSE = 1

pyautogui.hotkey("Ctrl", "t")

link = ("https://www.angelicoanziutti.com/post/responsabilidades-na-terceiriza%C3%A7%C3%A3o")
pyperclip.copy(link)
pyautogui.hotkey("Ctrl", "v")
pyautogui.press ("Enter")

pyautogui.PAUSE = 1

pyautogui.hotkey("Ctrl", "t")

link = 'https://www.angelicoanziutti.com/post/introdu%C3%A7%C3%A3o-%C3%A0-lei-geral-de-prote%C3%A7%C3%A3o-de-dados-pessoais-lgpd'
pyperclip.copy(link)
pyautogui.hotkey("Ctrl", "v")
pyautogui.press ("Enter")


pyautogui.PAUSE = 1

pyautogui.hotkey("Ctrl", "t")

link = ("https://www.angelicoanziutti.com/post/pens%C3%A3o-aliment%C3%ADcia-at%C3%A9-onde-vai-o-direito-do-meu-filho")
pyperclip.copy(link)
pyautogui.hotkey("Ctrl", "v")
pyautogui.press ("Enter")

